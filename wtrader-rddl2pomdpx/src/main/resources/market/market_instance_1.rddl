non-fluents market_nf {
	domain = market_domain;
	objects {
		computer : {c1,c2};
	};
	non-fluents {
		REBOOT-PROB = 0.02;
		CONNECTED(c1,c2);
	};
}

instance market_instance_1 {
	domain = market_domain;
	non-fluents = market_nf;
	
	init-state {
		running(c1);
		running(c2);
	};

	max-nondef-actions = 1;
	horizon  = 40;
	discount = 1.0;
}
